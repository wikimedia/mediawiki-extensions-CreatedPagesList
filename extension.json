{
	"name": "CreatedPagesList",
	"version": "1.2.1",
	"author": "Edward Chernenko",
	"url": "https://www.mediawiki.org/wiki/Extension:CreatedPagesList",
	"descriptionmsg": "createdpageslist-desc",
